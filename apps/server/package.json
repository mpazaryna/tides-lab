{
  "name": "tides",
  "version": "1.6.0",
  "description": "Tides (built with ModelFetch)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "WRANGLER_LOG_PATH=./logs wrangler dev src/index.ts --config wrangler.jsonc",
    "deploy": "WRANGLER_LOG_PATH=./logs wrangler deploy src/index.ts --config wrangler.jsonc",
    "delete": "WRANGLER_LOG_PATH=./logs wrangler delete",
    "types": "WRANGLER_LOG_PATH=./logs wrangler types --config wrangler.jsonc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:unit:watch": "jest tests/unit --watch",
    "test:integration:watch": "jest tests/integration --watch",
    "monitor": "node scripts/monitoring/metrics-fetcher.js",
    "monitor:simple": "node scripts/monitoring/simple-monitor.js",
    "monitor:live": "./scripts/monitoring/live-monitor.sh",
    "monitor:alerts": "node scripts/monitoring/monitor-alerts.js",
    "docs:generate": "typedoc --out docs/generated --entryPoints src/index.ts src/server.ts src/tools/index.ts src/handlers/*.ts",
    "docs:tools": "typedoc --out docs/generated/tools --entryPoints src/tools/tides.ts",
    "docs:server": "typedoc --out docs/generated/server --entryPoints src/server.ts",
    "deploy:prod": "WRANGLER_LOG_PATH=./logs wrangler deploy src/index.ts --config wrangler.jsonc --env 001",
    "deploy:staging": "WRANGLER_LOG_PATH=./logs wrangler deploy src/index.ts --config wrangler.jsonc --env 002",
    "deploy:dev": "WRANGLER_LOG_PATH=./logs wrangler deploy src/index.ts --config wrangler.jsonc --env 003",
    "deploy:006": "WRANGLER_LOG_PATH=./logs wrangler deploy src/index.ts --config wrangler.jsonc --env 006",
    "dev:prod": "WRANGLER_LOG_PATH=./logs wrangler dev src/index.ts --config wrangler.jsonc --env 001",
    "dev:staging": "WRANGLER_LOG_PATH=./logs wrangler dev src/index.ts --config wrangler.jsonc --env 002",
    "dev:dev": "WRANGLER_LOG_PATH=./logs wrangler dev src/index.ts --config wrangler.jsonc --env 003",
    "db:create:001": "WRANGLER_LOG_PATH=./logs wrangler d1 create tides-001-db",
    "db:create:002": "WRANGLER_LOG_PATH=./logs wrangler d1 create tides-002-db",
    "db:create:003": "WRANGLER_LOG_PATH=./logs wrangler d1 create tides-003-db",
    "secret:set:001": "WRANGLER_LOG_PATH=./logs wrangler secret put CLOUDFLARE_API_TOKEN --env 001",
    "secret:set:002": "WRANGLER_LOG_PATH=./logs wrangler secret put CLOUDFLARE_API_TOKEN --env 002",
    "secret:set:003": "WRANGLER_LOG_PATH=./logs wrangler secret put CLOUDFLARE_API_TOKEN --env 003"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.0",
    "@modelfetch/cloudflare": "0.15.2",
    "tslib": "2.8.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/ws": "^8.18.1",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "typedoc": "^0.28.8",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "5.8.3",
    "wrangler": "4.26.0",
    "ws": "^8.18.3"
  }
}