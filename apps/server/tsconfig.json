{
  "include": ["worker-configuration.d.ts", "src", "../agents"],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "es2022",
    "lib": ["es2023"],
    "outDir": "dist",
    "strict": true,
    "incremental": true,
    "verbatimModuleSyntax": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@agents/*": ["../agents/*"],
      "@agents": ["../agents"]
    }
  }
}
